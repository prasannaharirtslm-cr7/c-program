#include <stdio.h>

int main() {
    int N;
    scanf("%d", &N);

    int start = 1; 

    for (int i = N; i >= 1; i--) {
        int current = start;

        for (int j = 1; j <= i; j++) {
            printf("%d", current);
            current = 1 - current;
        }

        printf("\n");
        start = 1 - start;
    }

    return 0;
}
